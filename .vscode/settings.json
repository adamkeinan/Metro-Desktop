		"cpp": "(R\"\\()([\\s\\S]*?)(\\)\")",
		"csharp": "(@\")([^\"]*(?:\"\"[^\"]*)*)(\")",
		"d": "(`)([\\s\\S]*?)(`)",
		"dart": "(''')([\\s\\S]*?)(''')",
		"eiffel": "(\"\\[)([\\s\\S]*?)(\\]\")",
		"elixir": "(\"\"\")([\\s\\S]*?)(\"\"\")",
		"elm": "(\"\"\")([\\s\\S]*?)(\"\"\")",
		"erlang": "(\")([^\"\\\\]*(?:\\\\[\\s\\S][^\"\\\\]*)*)(\")",
		"fsharp": "(\"\"\")([\\s\\S]*?)(\"\"\")",
		"go": "(`)([\\s\\S]*?)(`)",
		"groovy": "(''')([\\s\\S]*?)(''')",
		"html": "(`)([\\s\\S]*?)(`)",
		"javascriptreact": "(`)([\\s\\S]*?)(`)",
		"julia": "(\"\"\")([\\s\\S]*?)(\"\"\")",
		"kotlin": "(\"\"\")([\\s\\S]*?)(\"\"\")",
		"lua": "(\\[\\[)([\\s\\S]*?)(\\]\\])",
		"markdown": "(```.*)([\\s\\S]*?)(```)",
		"nim": "(\"\"\")([\\s\\S]*?)(\"\"\")",
		"objective-cpp": "(R\"\\()([\\s\\S]*?)(\\)\")",
		"ocaml": "(\")([^\"\\\\]*(?:\\\\[\\s\\S][^\"\\\\]*)*)(\")",
		"perl": "(<<['\"]?EOF['\"]?)([\\s\\S]*?)(EOF)",
		"perl6": "(:(to|heredoc)(:c)?[[/\"]EOF[[/\"];)([\\s\\S]*?)(EOF)",
		"php": "(<<<['\"]?EOD['\"]?)([\\s\\S]*?)(EOD)",
		"powershell": "(@\")([\\s\\S]*?)(\"@)",
		"python": "(\"\"\")([\\s\\S]*?)(\"\"\")",
		"r": "(')([^'\\\\]*(?:\\\\[\\s\\S][^'\\\\]*)*)(')",
		"racket": "(#<<EOF)([\\s\\S]*?)(EOF)",
		"razor": "(@\")([\\s\\S]*?)(\")",
		"ruby": "(<<EOF-?)([\\s\\S]*?)(EOF)",
		"rust": "(r#\")([\\s\\S]*?)(\"#)",
		"scala": "(\"\"\")([\\s\\S]*?)(\"\"\")",
		"shellscript": "(<< ?['\"]?EOF['\"]?)([\\s\\S]*?)(EOF)",
		"typescriptreact": "(`)([\\s\\S]*?)(`)",
