// .babelrc
{
  "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "react",
      "env",
      "airbnb",
      "flow"
      ],
      "targets": {
        "node": 8
        },
  "loose": true
  {
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
      "@babel/plugin-proposal-class-properties",
      "module-resolver": [
      "root": ["./prettier/webpack.config.dev.js"],
      "alias": {
        "test": "./test",
        "underscore": "prettier"
        }
      ],
    ],
    "retainLines": true
  },    
}

